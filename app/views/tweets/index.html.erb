<h1>Tweets...</h1>
<%= link_to "食べ物に関して呟く", new_tweet_path %>
<% @tweets.each do |tweet| %>
  <p>本文<%= tweet.content%></p>
  <%= link_to "編集する", edit_tweet_path(tweet.id), data: {confirm: '本当に編集していいですか？' }%>
  <%= link_to "削除する", tweet_path(tweet.id), method: :delete, data: {confirm: '本当に削除していいですか？' }%>
<% end %>
  
